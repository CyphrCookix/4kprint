{
    "builds":  [
                   {
                       "src":  "index.html",
                       "use":  "@vercel/static"
                   }
               ],
    "headers":  [
                    {
                        "headers":  [
                                        {
                                            "key":  "X-Frame-Options",
                                            "value":  "DENY"
                                        },
                                        {
                                            "key":  "X-Content-Type-Options",
                                            "value":  "nosniff"
                                        }
                                    ],
                        "source":  "/(.*)"
                    }
                ],
    "routes":  [
                   {
                       "src":  "/(.*)",
                       "dest":  "/index.html"
                   }
               ],
    "version":  2
}
